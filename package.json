{
  "name": "nextjs-boilerplate-antd",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:windows": "npx env-cmd -x -f ./.env.local next dev -p $PORT",
    "dev": "npx env-cmd -x -f ./.env.local next dev -p '$PORT'",
    "start:windows": "npx env-cmd -x -f ./.env.local next start -p $PORT",
    "start": "npx env-cmd -x next start -p '$PORT'",
    "build": "next build",
    "lint": "next lint",
    "prettier-all": "npx prettier --config .prettierrc --write ./src",
    "fix": "npx eslint --fix ./src --ext .js --ext .jsx",
    "dependencies-upgrade": "yarn yarn-upgrade-all"
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "@monaco-editor/react": "^4.6.0",
    "@slider-captcha/core": "^1.0.1",
    "@slider-captcha/react": "^1.0.0",
    "ajv": "^8.12.0",
    "antd": "^5.12.6",
    "axios": "^1.6.3",
    "case": "^1.6.3",
    "dayjs": "^1.11.10",
    "iron-session": "^8.0.1",
    "lodash": "^4.17.21",
    "lowdb": "^7.0.1",
    "next": "14.0.4",
    "next-http-proxy-middleware": "^1.2.6",
    "pdfmake": "^0.2.9",
    "prop-types": "^15.8.1",
    "qs": "^6.11.2",
    "react": "^18",
    "react-dom": "^18",
    "request": "^2.88.2",
    "styled-components": "^6.1.6",
    "use-media-antd-query": "^1.1.0"
  },
  "devDependencies": {
    "@ant-design/cssinjs": "^1.18.2",
    "env-cmd": "^10.1.0",
    "eslint": "^8.44.0",
    "eslint-config-next": "^13.4.9",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-unicorn": "^50.0.1",
    "eslint-plugin-unused-imports": "^3.0.0",
    "prettier": "^3.1.1",
    "yarn-upgrade-all": "^0.7.2"
  },
  "yarn-upgrade-all": {
    "ignore": [
      "react",
      "next",
      "react-dom"
    ]
  }
}
